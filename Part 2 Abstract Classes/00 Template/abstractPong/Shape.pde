abstract class Shape {
  //Global Variables
  float x; //X-Variable, Position
  float y; //Y-Variable, Position
  float w; //Width
  float h; //Height
  color c; //Shape Colour
  //
  //Constructor
  Shape(float xParameter, float yParameter, float wParameter, float hParameter, color cParameter) {
    this.x;
    this.y;
    this.w;
    this.h;
    this.c;
  } //End Constructor
  //
  //Methods
  //
} //End Shape
